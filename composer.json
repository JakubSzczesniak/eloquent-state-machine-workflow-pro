{
    "name": "jakubszczesniak/eloquent-state-machine-workflow-pro",
    "description": "State machines for your Laravel Eloquent models",
    "keywords": [
        "jakubszczesniak",
        "laravel-eloquent-state-machines",
        "state-machine"
    ],
    "homepage": "https://github.com/asantibanez/laravel-eloquent-state-machines",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jakub Szcze≈õniak",
            "email": "jakub.szczesniak@protonmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "javoscript/laravel-macroable-models": "^1.0"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.0.4",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "JakubSzczesniak\\LaravelEloquentStateMachines\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JakubSzczesniak\\LaravelEloquentStateMachines\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "JakubSzczesniak\\LaravelEloquentStateMachines\\LaravelEloquentStateMachinesServiceProvider"
            ]
        }
    }
}
